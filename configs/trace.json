{
  "pipeline": {
    "mode": "iterative",
    "repeats": 120,
    "warmup_iter": 10,
    "fixed_clock": true,
    "runner": "icd.runtime.runners.mock_inference",
    "runner_context": {
      "tokens": 512,
      "linear_delay_s": 0.00012,
      "iter_delay_s": 0.0001,
      "provide_l2": true,
      "provide_ept": true
    }
  },
  "graph": {"source": "trace", "trace": "tests/data/trace_demo.jsonl", "normalize": "row"},
  "solver": {"time_budget_s": 1.0, "refine_steps": 200, "k_blocks": 4, "rng_seed": 0},
  "pair": {
    "baseline_methods": ["linear", "louvain", "memory_aware", "hardware_aware"]
  },
  "measure": {"ncu_enable": false, "power_enable": false},
  "report": {"out_dir": "runs/trace_iter"}
}
