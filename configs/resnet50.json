{
  "pipeline": {
    "mode": "iterative",
    "repeats": 200,
    "warmup_iter": 20,
    "fixed_clock": true,
    "runner": "icd.runtime.runners.mock_inference",
    "runner_context": {
      "tokens": 128,
      "model_loader": "icd.experiments.vision.load_torchvision_resnet50",
      "model_loader_kwargs": {
        "weights": "IMAGENET1K_V2",
        "batch_size": 4,
        "image_size": 224,
        "device": "cuda"
      }
    }
  },
  "graph": {
    "source": "pytorch",
    "normalize": "sym",
    "loader": "icd.experiments.vision.load_torchvision_resnet50",
    "loader_kwargs": {
      "weights": "IMAGENET1K_V2",
      "batch_size": 4,
      "image_size": 224,
      "device": "cuda"
    },
    "correlation": {
      "enable": true,
      "samples": 4,
      "normalize": "sym",
      "whiten": true,
      "transfer_batch_size": 2
    },
    "pytorch": {
      "hops": 2,
      "byte_weight": true,
      "reuse_decay": 0.6,
      "max_len": 256
    },
    "nnz_cap": 1000000
  },
  "solver": {
    "time_budget_s": 30,
    "refine_steps": 500,
    "k_blocks": 4,
    "rng_seed": 0
  },
  "pair": {
    "baseline_methods": ["linear", "louvain", "memory_aware", "hardware_aware"]
  },
  "transform": {
    "sparsity": {"enable": false},
    "quant": {"enable": false},
    "kv": {"enable": false}
  },
  "measure": {
    "ncu_enable": false,
    "power_enable": false,
    "nvtx_enable": true
  },
  "quality": {
    "enable": false
  },
  "report": {
    "out_dir": "runs/resnet50_smoke",
    "formats": ["html", "csv"]
  }
}
