{
  "pipeline": {
    "mode": "iterative",
    "repeats": 400,
    "warmup_iter": 40,
    "fixed_clock": true,
    "runner": "icd.runtime.runners.mock_inference",
    "runner_context": {
      "tokens": 4096,
      "model_loader": "icd.experiments.graph.load_pyg_gcn",
      "model_loader_kwargs": {
        "dataset_name": "ogbn-arxiv",
        "hidden_channels": 256,
        "num_layers": 3,
        "dropout": 0.5,
        "device": "cuda"
      }
    }
  },
  "graph": {
    "source": "pytorch",
    "normalize": "sym",
    "loader": "icd.experiments.graph.load_pyg_gcn",
    "loader_kwargs": {
      "dataset_name": "ogbn-arxiv",
      "hidden_channels": 256,
      "num_layers": 3,
      "dropout": 0.5,
      "device": "cuda"
    },
    "correlation": {
      "enable": true,
      "samples": 16,
      "normalize": "sym",
      "whiten": true,
      "transfer_batch_size": 4
    },
    "pytorch": {
      "hops": 2,
      "reuse_decay": 0.7,
      "byte_weight": true,
      "max_len": 256
    },
    "nnz_cap": 1500000
  },
  "solver": {
    "time_budget_s": 45,
    "refine_steps": 600,
    "k_blocks": 4,
    "rng_seed": 0
  },
  "pair": {
    "baseline_methods": ["linear", "louvain", "memory_aware", "hardware_aware"]
  },
  "transform": {
    "sparsity": {"enable": false},
    "quant": {"enable": false},
    "kv": {"enable": false}
  },
  "measure": {
    "ncu_enable": false,
    "power_enable": false,
    "nvtx_enable": true
  },
  "quality": {
    "enable": false
  },
  "report": {
    "out_dir": "runs/gcn_arxiv_smoke",
    "formats": ["html", "csv"]
  }
}
