{
  "task": "wt103",
  "pipeline": {
    "mode": "iterative",
    "repeats": 1000,
    "warmup_iter": 50,
    "fixed_clock": true,
    "runner": "icd.runtime.runners_hf.hf_causal_lm_runner",
    "runner_context": {
      "model_loader": "icd.experiments.hf.load_hf_causal_lm",
      "model_loader_kwargs": {
