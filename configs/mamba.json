        "prompt": [
          "The quick brown fox jumps over the lazy dog",
          "In a distant galaxy, explorers uncovered"
        ],
        "device": "cuda"
      }
    }
  },
  "graph": {
    "source": "pytorch",
    "normalize": "sym",
    "loader": "icd.experiments.hf.load_hf_causal_lm",
