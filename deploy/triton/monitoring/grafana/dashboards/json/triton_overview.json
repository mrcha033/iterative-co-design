{
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "rate(nv_inference_request_success[1m])",
          "legendFormat": "Success RPS"
        }
      ],
      "title": "Inference Requests",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(nv_inference_request_duration_us_bucket[5m])) by (le)) / 1000",
          "legendFormat": "p95 latency"
        }
      ],
      "title": "Latency",
      "type": "timeseries"
    }
  ],
  "schemaVersion": 37,
  "tags": ["triton"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-15m",
